<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="savedLocationVM" type="com.hive.app.modules.savedlocation.data.viewmodel.SavedLocationVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSavedLocation"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnshape"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearStatusBar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageShape"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_shape"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_shape"/>
                <LinearLayout android:id="@+id/linearRowshapeOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_233pxh"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageShapeOne"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900"/>
                    <ImageView android:id="@+id/imageShapeTwo"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900_10x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900_10x15"/>
                    <FrameLayout android:id="@+id/frameStackpath"
                        android:layout_height="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_22pxh">
                        <ImageView android:id="@+id/imagePath"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path"/>
                        <ImageView android:id="@+id/imagePathOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_7pxh"
                            android:layout_marginHorizontal="@dimen/_2pxh"
                            android:layout_marginVertical="@dimen/_2pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imagePathTwo"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_path_black_900_4x1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900_4x1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSavedLocation"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_78pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{savedLocationVM.savedLocationModel.txtSavedLocation}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_saved_location"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowgrouptwelve"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_teal_300_radius_20"
                    android:id="@+id/btnGroupTwelve"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group8" tools:ignore="ContentDescription"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHome"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{savedLocationVM.savedLocationModel.txtHome}"
                        style="@style/txtPoppinssemibold14_2" tools:text="@string/lbl_home"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCountry"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{savedLocationVM.savedLocationModel.txtCountry}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_jerash_jordan"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <View android:background="@color/black_900_63"
            android:id="@+id/lineDivider"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_315pxh"/>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="start"
            android:id="@+id/recyclerSavedLocation"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_425pxv"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_33pxh}" tools:itemCount="3" tools:listitem="@layout/row_saved_location"/>
    </LinearLayout>
</layout>
