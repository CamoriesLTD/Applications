<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchPageVM" type="com.hive.app.modules.searchpage.data.viewmodel.SearchPageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnshape"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearSearchPage"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearNewLocation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <LinearLayout android:id="@+id/linearStatusBar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageShape"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape"/>
                    <LinearLayout android:id="@+id/linearRowshapeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_233pxh"
                        android:layout_weight="0.71"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageShapeOne"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_shape_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900"/>
                        <ImageView android:id="@+id/imageShapeTwo"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_shape_black_900_10x15"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900_10x15"/>
                        <FrameLayout android:id="@+id/frameStackpath"
                            android:layout_height="@dimen/_11pxv"
                            android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_22pxh">
                            <ImageView android:id="@+id/imagePath"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_width="@dimen/_22pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_path"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_path"/>
                            <ImageView android:id="@+id/imagePathOne"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_7pxh"
                                android:layout_marginHorizontal="@dimen/_2pxh"
                                android:layout_marginVertical="@dimen/_2pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_path_black_900"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900"/>
                        </FrameLayout>
                        <ImageView android:id="@+id/imagePathTwo"
                            android:layout_height="@dimen/_4pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_1pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path_black_900_4x1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900_4x1"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearLocation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_2">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFrom"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{searchPageVM.searchPageModel.txtFrom}"
                        style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_from"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCurrentlocatio"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_140pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{searchPageVM.searchPageModel.txtCurrentlocatio}"
                        style="@style/txtPoppinsregular14_1" tools:text="@string/msg_current_locatio"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearDestination"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_2">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.14"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{searchPageVM.searchPageModel.txtTo}"
                        style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_to"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnterDestinati"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_155pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{searchPageVM.searchPageModel.txtEnterDestinati}"
                        style="@style/txtPoppinsregular14_2" tools:text="@string/msg_enter_destinati"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSavedLocations"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListgroupeight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_33pxh}" tools:itemCount="2" tools:listitem="@layout/row_listgroupeight"/>
                <View android:background="@color/black_900_63"
                    android:id="@+id/lineDivider"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_315pxh"/>
                <LinearLayout android:id="@+id/linearSavedLocations1"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowgroupsix"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_teal_300_radius_20"
                            android:id="@+id/btnGroupSix"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:paddingHorizontal="@dimen/_12pxh"
                            android:paddingVertical="@dimen/_12pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_group6" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAllSavedLocat"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_weight="0.77"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@{searchPageVM.searchPageModel.txtAllSavedLocat}"
                            style="@style/txtPoppinssemibold14_2" tools:text="@string/msg_all_saved_locat"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_113pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRecentlocation"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRecentlocation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchPageVM.searchPageModel.txtRecentlocation}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/msg_recent_location"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListgroupfive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_43pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_32pxh}" tools:itemCount="4" tools:listitem="@layout/row_listgroupfive"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
