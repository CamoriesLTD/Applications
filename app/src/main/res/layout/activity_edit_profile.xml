                    android:id="@+id/txtEditProfile"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_98pxh"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtEditProfile}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_edit_profile"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackprofilepicture"
