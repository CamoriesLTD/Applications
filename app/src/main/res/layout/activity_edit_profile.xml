                    android:id="@+id/txtUserName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtUserName}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_user_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_bluegray_900_19_radius_8"
