<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homePageVM" type="com.hive.app.modules.homepage.data.viewmodel.HomePageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearHomePage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start" android:id="@+id/frameMap"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
            <LinearLayout android:id="@+id/linearColumncar"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    app:contentInsetStart="0dp" style="@style/groupStyle">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnshape"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <LinearLayout android:id="@+id/linearStatusBar"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageShape"
                                android:layout_height="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_27pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_shape_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_white_a700"/>
                            <LinearLayout
                                android:id="@+id/linearRowshapeOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_233pxh"
                                android:layout_weight="0.71"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageShapeOne"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_width="@dimen/_17pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_shape_white_a700_10x17"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_shape_white_a700_10x17"/>
                                <ImageView
                                    android:id="@+id/imageShapeTwo"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_shape_white_a700_10x15"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_shape_white_a700_10x15"/>
                                <FrameLayout
                                    android:id="@+id/frameStackpath"
                                    android:layout_height="@dimen/_11pxv"
                                    android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_22pxh">
                                    <ImageView
                                    android:id="@+id/imagePath"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_11pxh"
                                    android:layout_width="@dimen/_22pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_path_white_a700_11x22"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_path_white_a700_11x22"/>
                                    <ImageView
                                    android:id="@+id/imagePathOne"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_7pxh"
                                    android:layout_marginHorizontal="@dimen/_2pxh"
                                    android:layout_marginVertical="@dimen/_2pxv"
                                    android:layout_width="@dimen/_18pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_path_white_a700_7x18"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_path_white_a700_7x18"/>
                                </FrameLayout>
                                <ImageView
                                    android:id="@+id/imagePathTwo"
                                    android:layout_height="@dimen/_4pxh"
                                    android:layout_marginBottom="@dimen/_3pxv"
                                    android:layout_marginStart="@dimen/_1pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_width="@dimen/_1pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_path_white_a700_4x1"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_path_white_a700_4x1"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowicon"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_30pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIcon"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginBottom="@dimen/_12pxv"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_white_a700"/>
                            <ImageView
                                android:id="@+id/imageProfilePicture"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_marginStart="@dimen/_247pxh"
                                android:layout_width="@dimen/_40pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_profilepicture"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_profilepicture"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_83pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCar"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_car"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_car"/>
                    <ImageView android:id="@+id/imageCarOne"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_183pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_car_21x35"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_car_21x35"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMyLocation"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_56pxh"
                    android:layout_marginEnd="@dimen/_63pxh"
                    android:layout_marginStart="@dimen/_63pxh"
                    android:layout_marginTop="@dimen/_63pxv"
                    android:layout_width="@dimen/_56pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mylocation"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_mylocation"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowcarTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCarTwo"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_car_36x25"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_car_36x25"/>
                    <ImageView android:id="@+id/imageCarThree"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_153pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_car_26x36"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_car_26x36"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowcarFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCarFour"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_car_36x27"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_car_36x27"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_teal_300_radius_20"
                        android:id="@+id/btnLocationIcon"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_237pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_locationicon" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnwherewouldyou"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWherewouldyou"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{homePageVM.homePageModel.txtWherewouldyou}"
                        style="@style/txtPoppinssemibold14_2" tools:text="@string/msg_where_would_you"/>
                    <androidx.appcompat.widget.SearchView
                        android:background="@drawable/rectangle_bg_white_a700_border_black_900_1a_radius_10"
                        android:gravity="center_horizontal"
                        android:id="@+id/searchViewSearchField"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:theme="@style/searchViewSearchField"
                        app:iconifiedByDefault="false"
                        app:queryBackground="@android:color/transparent"
                        app:queryHint="@string/msg_enter_destinati" app:searchIcon="@drawable/img_search"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowshapeThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_30pxv"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearWork"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStyleteal_300_67cornerRadius">
                            <ImageView android:id="@+id/imageShapeThree"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_25pxh"
                                android:layout_marginEnd="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_29pxh"
                                android:layout_marginTop="@dimen/_18pxv"
                                android:layout_width="@dimen/_26pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_shape_teal_300"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_teal_300"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtWork"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginEnd="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_29pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_7pxh"
                                android:singleLine="true"
                                android:text="@{homePageVM.homePageModel.txtWork}"
                                style="@style/txtPoppinssemibold14_1" tools:text="@string/lbl_work"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearHome"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStyleteal_300_67cornerRadius">
                            <ImageView android:id="@+id/imagePathThree"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_22pxh"
                                android:layout_marginEnd="@dimen/_26pxh"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_path_teal_300_22x24"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_path_teal_300_22x24"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHome"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_17pxv"
                                android:layout_marginEnd="@dimen/_26pxh"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_7pxh"
                                android:singleLine="true"
                                android:text="@{homePageVM.homePageModel.txtHome}"
                                style="@style/txtPoppinssemibold14_1" tools:text="@string/lbl_home"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearRecently"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStyleteal_300_67cornerRadius">
                            <ImageView android:id="@+id/imageShapeFour"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_13pxh"
                                android:layout_marginEnd="@dimen/_17pxh"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_25pxv"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_shape_teal_300_13x24"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_teal_300_13x24"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtRecently"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_7pxh"
                                android:singleLine="true"
                                android:text="@{homePageVM.homePageModel.txtRecently}"
                                style="@style/txtPoppinssemibold14_1" tools:text="@string/lbl_recently"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
