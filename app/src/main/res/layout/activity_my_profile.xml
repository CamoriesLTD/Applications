<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myProfileVM" type="com.hive.app.modules.myprofile.data.viewmodel.MyProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameMyProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnshape"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearStatusBar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageShape"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_shape"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_shape"/>
                <LinearLayout android:id="@+id/linearRowshapeOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_233pxh"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageShapeOne"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900"/>
                    <ImageView android:id="@+id/imageShapeTwo"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900_10x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900_10x15"/>
                    <FrameLayout android:id="@+id/frameStackpath"
                        android:layout_height="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_22pxh">
                        <ImageView android:id="@+id/imagePath"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path"/>
                        <ImageView android:id="@+id/imagePathOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_7pxh"
                            android:layout_marginHorizontal="@dimen/_2pxh"
                            android:layout_marginVertical="@dimen/_2pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imagePathTwo"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_path_black_900_4x1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900_4x1"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_41pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMyProfile"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100pxh"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{myProfileVM.myProfileModel.txtMyProfile}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_my_profile"/>
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_teal_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_teal_300"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageProfilePicture"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_100pxh"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_profilepicture}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_profilepicture"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUserName"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:singleLine="true"
                android:text="@{myProfileVM.myProfileModel.txtUserName}"
                style="@style/txtPoppinsbold16" tools:text="@string/lbl_tanya_robinson"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumniconOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearMyOrders"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_41pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowiconOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconOne"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_teal_300_19x18"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_teal_300_19x18"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMytrips"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_weight="0.78"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtMytrips}"
                            style="@style/txtPoppinssemibold16" tools:text="@string/lbl_my_trips"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_175pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </LinearLayout>
                <View android:background="@color/black_900_63"
                    android:id="@+id/lineDivider"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_315pxh"/>
                <LinearLayout android:id="@+id/linearFavourites"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowiconTwo"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.96"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconTwo"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_teal_300_17x20"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_teal_300_17x20"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSavedLocations"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_weight="0.87"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtSavedLocations}"
                            style="@style/txtPoppinssemibold16" tools:text="@string/lbl_saved_locations"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightOne"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_107pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </LinearLayout>
                <View android:background="@color/black_900_63"
                    android:id="@+id/lineDividerOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_315pxh"/>
                <LinearLayout android:id="@+id/linearPayment"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowiconThree"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconThree"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_teal_300_16x20"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_teal_300_16x20"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPayment"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_weight="0.79"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtPayment}"
                            style="@style/txtPoppinssemibold16" tools:text="@string/lbl_payment"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightTwo"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_166pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </LinearLayout>
                <View android:background="@color/black_900_63"
                    android:id="@+id/lineDividerTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_315pxh"/>
                <LinearLayout android:id="@+id/linearSettings"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_221pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowiconFour"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconFour"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_teal_300_20x19"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_teal_300_20x19"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettings"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_weight="0.77"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtSettings}"
                            style="@style/txtPoppinssemibold16" tools:text="@string/lbl_settings"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightThree"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_173pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
