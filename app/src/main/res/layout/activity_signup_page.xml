<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signupPageVM" type="com.hive.app.modules.signuppage.data.viewmodel.SignupPageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignupPage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnshape"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearStatusBar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageShape"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_shape"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_shape"/>
                <LinearLayout android:id="@+id/linearRowshapeOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_233pxh"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageShapeOne"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900"/>
                    <ImageView android:id="@+id/imageShapeTwo"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shape_black_900_10x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_black_900_10x15"/>
                    <FrameLayout android:id="@+id/frameStackpath"
                        android:layout_height="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_22pxh">
                        <ImageView android:id="@+id/imagePath"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path"/>
                        <ImageView android:id="@+id/imagePathOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_7pxh"
                            android:layout_marginHorizontal="@dimen/_2pxh"
                            android:layout_marginVertical="@dimen/_2pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_path_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imagePathTwo"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_path_black_900_4x1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_path_black_900_4x1"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtCreateAccount"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_12pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtCreateAccount}"
                style="@style/txtPoppinsbold24" tools:text="@string/lbl_create_account"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtConnectby"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_41pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtConnectby}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_connect_by"/>
            <LinearLayout android:id="@+id/linearRowicon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearGoogle"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_38pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGoogle"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_33pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{signupPageVM.signupPageModel.txtGoogle}"
                        style="@style/txtPoppinsmedium14" tools:text="@string/lbl_google"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearFacebook"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyleindigo_500cornerRadius">
                    <ImageView android:id="@+id/imagePathThree"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_path_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_path_white_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFacebook"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{signupPageVM.signupPageModel.txtFacebook}"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_facebook"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtOrcreateaccou"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtOrcreateaccou}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/msg_or_create_accou"/>
            <TextView android:ellipsize="end" android:id="@+id/txtName"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtName}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_name"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_bluegray_900_19_radius_8"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_your_name"
                android:id="@+id/etGroupFortyFour"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={signupPageVM.signupPageModel.etGroupFortyFourValue}"
                android:textColorHint="@color/bluegray_900_7e"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtEmail}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_email"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_bluegray_900_19_radius_8"
                android:ellipsize="end"
                android:hint="@string/msg_enter_your_emai"
                android:id="@+id/etGroupFortyThree"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={signupPageVM.signupPageModel.etGroupFortyThreeValue}"
                android:textColorHint="@color/bluegray_900_7e"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{signupPageVM.signupPageModel.txtPassword}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_password"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_bluegray_900_19_radius_8"
                android:drawableEnd="@drawable/img_icon_bluegray_900"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/msg_enter_your_pass"
                android:id="@+id/etGroupFortyTwo"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_21pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={signupPageVM.signupPageModel.etGroupFortyTwoValue}"
                android:textColorHint="@color/bluegray_900_7e"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnCreateAccountOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_create_account"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_create_account"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowalreadyhaveac"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_109pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAlreadyhaveac"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.81"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    android:text="@{signupPageVM.signupPageModel.txtAlreadyhaveac}"
                    style="@style/txtPoppinsregular14_1" tools:text="@string/msg_already_have_ac"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogin"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    android:text="@{signupPageVM.signupPageModel.txtLogin}"
                    style="@style/txtPoppinsbold14" tools:text="@string/lbl_login"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
